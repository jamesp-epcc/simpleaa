<web-app>
  <display-name>SAFE Attribute Authority Service</display-name>
  <servlet>
    <servlet-name>AttributeAuthorityServlet</servlet-name>
    <servlet-class>uk.ac.ed.epcc.safeaa.AttributeAuthorityServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AttributeAuthorityServlet</servlet-name>
    <url-pattern>/*</url-pattern>
  </servlet-mapping>
</web-app>
